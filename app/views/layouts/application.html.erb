<!DOCTYPE html>
<html>
	<head>
  		<title>Triton Yelp</title>
  		<%= stylesheet_link_tag    "application", :media => "all" %>
  		<%= javascript_include_tag "application" %>
  		<%= csrf_meta_tags %>
	</head>
	<body>
		<div class="container">
			<%= render 'layouts/header' %>
			<section class ="round">
				<p id="notice"><%= notice %></p>
				<%= yield %>
			</section>
			<%= render 'layouts/footer' %>
		</div>
	</body>
</html>
