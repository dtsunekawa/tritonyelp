<h1>Listing Reviews</h1>

<table>
	<tr>
		<th>Name</th>
	</tr>
<%= reviews.each do |review| %>
	<tr>
		<td><%= review.id %></td>
		<td><%= link_to 'Show', review %></td>
		<td><%= link_to 'Edit', edit_review_path(review) %></td>
		<td><%= link_to 'Delete', store, :method => :delete, :data => { :confirm => 'Are you sure you\'d like to delete this review?' } %></td>
	</tr>
<% end %>
</table>

<br />

<%= link_to 'New Review', new_review_path %>